{
  "name": "SwissLife_Angular_Electron_Prototype",
  "version": "0.0.1",
  "description": "A minimal application using Electron and AngularJS. \n Based on https://github.com/electron/electron-quick-start.",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --all",
    "run": "http-server -a localhost -p 8000 -c-1 ./dist/app",
    "exe:linux64": "electron-packager ./dist/ --platform=linux --arch=x64",
    "exe:win64": "electron-packager ./dist/ --platform=win32 --arch=x64",
    "msi": "gulp msi"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "PQ Warlot",
  "license": "",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.2",
    "electron": "^1.4.1",
    "electron-builder": "^8.6.0",
    "electron-packager": "^8.3.0",
    "electron-prebuilt": "^1.4.8",
    "electron-winstaller": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-debug": "^3.0.0",
    "gulp-inject": "^4.1.0",
    "gulp-livereload": "^3.8.1",
    "gulp-main-bower-files": "^1.5.3",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^2.3.2",
    "gulp-usemin": "^0.3.25",
    "jasmine-core": "^2.5.2",
    "jasmine-reporters": "^2.2.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "mkdirp": "^0.5.1",
    "protractor": "^4.0.11",
    "run-sequence": "^1.2.2",
    "uuid": "^3.0.0"
  },
  "dependencies": {
    "electron-prebuilt": "^1.4.8",
    "wiredep": "^4.0.0"
  },
  "build": {
    "appId": "fr.ebusinessinformation.angular.electron.prototype",
    "app-category-type": "public.app-category.productivity",
    "dmg": {},
    "win": {
      "iconUrl": "http://www.swisslife.fr/extension/mcms/design/mcms/images/icon/favicon.ico"
    }
  }
}
